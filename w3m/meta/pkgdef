[w3m]
pkgname="${pkgprefix}${topdir}"
name="$topdir - w3m is a text-based web browser as well as a pager like more or less"
pkgcat="application"
pkgvendor="https://w3m.sourceforge.net"
pkgdesc="w3m is text-basd web browser as well as pager"
pkgver="$pkgver"
files(-,root,bin)
default_docs
